{
  "expo": {
    "name": "gymshock_app",
    "slug": "gymshock",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "com.googleusercontent.apps.158995666507-ovl3lpferogh47jbddbuotme5auuitqm",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.gymshock",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.158995666507-ovl3lpferogh47jbddbuotme5auuitqm"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.gymshock",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "com.googleusercontent.apps.158995666507-ovl3lpferogh47jbddbuotme5auuitqm"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "enableFullScreenImage_Legacy": true,
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Permite acceso a la cámara para análisis de técnica en tiempo real",
          "enableMicrophonePermission": false
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Permite acceso a la cámara para grabar videos de ejercicios"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Permite guardar videos de tus entrenamientos",
          "savePhotosPermission": "Permite guardar videos procesados"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
